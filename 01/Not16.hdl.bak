/**
 * 16-bit Not:
 * for i=0..15: out[i] = not in[i]
 */

CHIP Not16 {
    IN in[16];
    OUT out[16];

    PARTS:
    Not (in=in[0], out=out[0]);
	Not (in=in[0], out=out[1]);
	Not (in=in[0], out=out[2]);
	Not (in=in[0], out=out[3]);
	Not (in=in[0], out=out[4]);
	Not (in=in[0], out=out[5]);
	Not (in=in[0], out=out[6]);
	Not (in=in[0], out=out[7]);
	Not (in=in[0], out=out[8]);
	Not (in=in[0], out=out[9]);
	Not (in=in[0], out=out[10]);
	Not (in=in[0], out=out[11]);
	Not (in=in[0], out=out[12]);
	Not (in=in[0], out=out[13]);
	Not (in=in[0], out=out[14]);
	Not (in=in[0], out=out[15]);
}